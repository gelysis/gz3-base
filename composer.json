{
    "name": "geolysis/gz3-base",
    "description": "A set of extended ZF3 base classes.",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf3"
    ],
    "homepage": "https://bitbucket.org/geolysis/gz3-base",
    "authors": [
        {
            "name": "Andreas Gerhards",
            "email": "geolysis@zoho.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.1-dev",
            "dev-develop": "0.9.1-dev"
        }
    },
    "require": {
        "php": "^7.0.7",
        "container-interop/container-interop": "^1.1",
        "phpunit/PHPUnit": ">=4.0",
        "zendframework/zend-mvc": "^3.0",
        "zendframework/zend-db": ">=2.8",
        "zendframework/zend-di": ">=2.6",
        "zendframework/zend-log": ">=2.9",
        "zendframework/zend-servicemanager": "^3.0.3",
        "zendframework/zend-test": "^3.0"
    },
    "autoload": {
            "psr-4": {
            "Gz3Base\\": "src/"
        }
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": ">=3.0"
    },
    "autoload-dev": {
            "psr-4": {
            "Gz3BaseTest\\": "test/"
        }
    }
}
